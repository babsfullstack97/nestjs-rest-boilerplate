{
    "name": "nestjs-rest-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "Hebert Barros <hebert@hotbrains.com.br>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
        "setup": "cp .env.example .env",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:migration:run": "npm run typeorm -- migration:run",
        "pretypeorm": "ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "run:pm2": "pm2 start ts-node -- -P tsconfig.json ./src/main.ts",
        "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "dev:test": "nodemon src/main.ts --debug --watch",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "git:ruf": "git reset --hard HEAD",
        "git:pull": "git pull",
        "kill:node": "kill -9 $(sudo lsof -t -i:3333)",
        "clear": "clear",
        "audit:fix": "npm audit fix"
    },
    "dependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/graphql": "^7.10.6",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/schedule": "^1.0.2",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^4.8.0",
        "@nestjs/throttler": "^2.0.0",
        "@nestjs/typeorm": "^7.1.5",
        "@sentry/node": "^6.13.3",
        "@sentry/tracing": "^6.13.3",
        "@vercel/node": "^1.9.1",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "beautlog": "^0.2.0",
        "body-parser": "^1.19.0",
        "cache-manager": "^3.4.4",
        "celebrate": "^15.0.0",
        "class-transformer": "0.4.0",
        "class-validator": "^0.13.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "data-crypto": "^1.4.4",
        "date-fns": "^2.25.0",
        "date-handle": "^0.0.1",
        "dotenv": "^8.2.0",
        "expmidd": "0.1.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "gerador-validador-cpf": "^5.0.2",
        "helmet": "^3.23.3",
        "http-status": "^1.4.2",
        "jsonwebtoken": "^8.5.1",
        "mailjet-abstraction": "^0.0.2",
        "moment": "^2.29.1",
        "mongoose": "^5.6.7",
        "morgan": "^1.10.0",
        "nestjs-pino-logger": "^0.1.0",
        "pg": "^8.6.0",
        "pgnode": "^0.0.6",
        "pino": "^6.13.3",
        "pino-http": "^5.8.0",
        "qs": "^6.10.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.6",
        "socket.io": "^4.3.0",
        "swagger-ui-express": "^4.1.6",
        "tsyringe": "^4.6.0",
        "typeorm": "^0.2.32",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.13.16",
        "@babel/node": "^7.13.13",
        "@babel/preset-env": "^7.13.15",
        "@compodoc/compodoc": "^1.0.9",
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.15",
        "@types/axios": "^0.14.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/cache-manager": "^3.4.2",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.10",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.12",
        "@types/helmet": "^0.0.45",
        "@types/jest": "^26.0.22",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/moment": "^2.13.0",
        "@types/mongoose": "^5.7.3",
        "@types/morgan": "^1.9.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^14.14.36",
        "@types/pg": "^8.6.0",
        "@types/supertest": "^2.0.10",
        "@types/uuidv4": "^5.0.0",
        "@types/yup": "^0.29.13",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "nodemon": "^2.0.14",
        "pm2": "^5.1.0",
        "prettier": "^2.2.1",
        "sucrase": "^3.12.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
