{
    "name": "nestjs-rest-boilerplate",
    "version": "0.0.2",
    "description": "Nestjs rest API boilerplate",
    "author": "Hebert Cisco <hebertcisco@outlook.com>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
        "setup": "cp .env.example .env",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:migration:run": "npm run typeorm -- migration:run",
        "pretypeorm": "ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:pm2": "pm2 start ts-node -- -P tsconfig.json ./src/main.ts",
        "start:doc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "dev": "nodemon src/main.ts --debug --watch",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare:enviroment": "node scripts/dev-enviroment.cjs",
        "prepostprepare:enviroment": "npm run pretypeorm"
    },
    "dependencies": {
        "@nestjs/common": "9.1.5",
        "@nestjs/core": "9.1.5",
        "@nestjs/mapped-types": "1.2.0",
        "@nestjs/platform-express": "9.1.5",
        "@nestjs/serve-static": "3.0.0",
        "@nestjs/swagger": "6.1.2",
        "@nestjs/throttler": "3.1.0",
        "@nestjs/typeorm": "9.0.1",
        "axios": "1.1.3",
        "bcryptjs": "2.4.3",
        "beautlog": "0.4.4",
        "body-parser": "1.20.1",
        "cache-manager": "4.1.0",
        "cache-manager-redis-store": "2.0.0",
        "chance": "1.1.9",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "16.0.3",
        "express": "4.18.2",
        "express-async-errors": "3.1.1",
        "helmet": "6.0.0",
        "http-status": "1.5.3",
        "ioredis": "5.2.3",
        "jsonwebtoken": "8.5.1",
        "moment": "2.29.4",
        "mongoose": "6.6.7",
        "morgan": "1.10.0",
        "nest-shared": "0.4.51",
        "pg": "8.8.0",
        "pgnode": "1.1.2",
        "qs": "6.11.0",
        "redis": "4.3.1",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.7",
        "swagger-ui-express": "4.5.0",
        "tsyringe": "4.7.0",
        "typeorm": "0.3.10",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "@babel/cli": "7.19.3",
        "@babel/core": "7.19.6",
        "@babel/node": "7.19.1",
        "@babel/preset-env": "7.19.4",
        "@compodoc/compodoc": "1.1.19",
        "@nestjs/cli": "9.1.4",
        "@nestjs/schematics": "9.0.3",
        "@nestjs/testing": "9.1.5",
        "@types/axios": "0.14.0",
        "@types/bcryptjs": "2.4.2",
        "@types/body-parser": "1.19.2",
        "@types/cache-manager": "4.0.2",
        "@types/cache-manager-redis-store": "2.0.1",
        "@types/chance": "1.1.3",
        "@types/compression": "1.7.2",
        "@types/cors": "2.8.12",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.14",
        "@types/helmet": "0.0.48",
        "@types/ioredis": "4.28.10",
        "@types/jest": "29.2.0",
        "@types/jsonwebtoken": "8.5.9",
        "@types/moment": "2.13.0",
        "@types/mongoose": "5.11.97",
        "@types/morgan": "1.9.3",
        "@types/multer": "1.4.7",
        "@types/node": "18.7.18",
        "@types/pg": "8.6.5",
        "@types/redis": "4.0.11",
        "@types/supertest": "2.0.12",
        "@types/uuidv4": "5.0.0",
        "@types/yup": "0.29.14",
        "@typescript-eslint/eslint-plugin": "5.40.1",
        "@typescript-eslint/parser": "5.40.1",
        "@typescript-eslint/typescript-estree": "5.40.1",
        "eslint": "8.26.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.2.1",
        "nodemon": "2.0.20",
        "pm2": "5.2.2",
        "prettier": "2.7.1",
        "sucrase": "3.28.0",
        "supertest": "6.3.0",
        "ts-jest": "29.0.3",
        "ts-loader": "9.4.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
